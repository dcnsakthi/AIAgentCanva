# Application Settings
APP_NAME=MAF Agent Builder
APP_ENV=development
SECRET_KEY=your-secret-key-here

# Azure OpenAI Settings
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/
AZURE_OPENAI_API_KEY=your-api-key
AZURE_OPENAI_DEPLOYMENT_NAME=gpt-4
AZURE_OPENAI_API_VERSION=2024-02-15-preview

# OpenAI Settings
OPENAI_API_KEY=your-openai-api-key

# Azure AI Services
AZURE_AI_ENDPOINT=https://your-ai-services.cognitiveservices.azure.com/
AZURE_AI_API_KEY=your-ai-api-key

# Azure Search
AZURE_SEARCH_ENDPOINT=https://your-search-service.search.windows.net
AZURE_SEARCH_API_KEY=your-search-api-key
AZURE_SEARCH_INDEX_NAME=agents-index

# Vector Database Settings
VECTOR_DB_TYPE=chromadb  # Options: chromadb, faiss, pinecone, qdrant, weaviate
PINECONE_API_KEY=your-pinecone-key
PINECONE_ENVIRONMENT=your-environment
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=your-qdrant-key
WEAVIATE_URL=http://localhost:8080

# Azure Cosmos DB
COSMOS_ENDPOINT=https://your-cosmos.documents.azure.com:443/
COSMOS_KEY=your-cosmos-key
COSMOS_DATABASE_NAME=maf_agents

# Azure Storage
AZURE_STORAGE_CONNECTION_STRING=your-connection-string
AZURE_STORAGE_CONTAINER_NAME=agent-assets

# Authentication
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret
MICROSOFT_CLIENT_ID=your-microsoft-client-id
MICROSOFT_CLIENT_SECRET=your-microsoft-client-secret
MICROSOFT_TENANT_ID=your-tenant-id
JWT_SECRET_KEY=your-jwt-secret

# Email Service (for magic links)
SENDGRID_API_KEY=your-sendgrid-key
EMAIL_FROM=noreply@yourdomain.com

# Deployment
AZURE_SUBSCRIPTION_ID=your-subscription-id
AZURE_RESOURCE_GROUP=maf-agents-rg
AZURE_LOCATION=eastus
CONTAINER_REGISTRY=youracr.azurecr.io
